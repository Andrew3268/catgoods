<h1>Jibsas</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jibsas.each do |jibsa| %>
      <tr>
        <td><%= jibsa.j_title %></td>
        <td><%= link_to 'Show', jibsa %></td>
        <td><%= link_to 'Edit', edit_jibsa_path(jibsa) %></td>
        <td><%= link_to 'Destroy', jibsa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jibsa', new_jibsa_path %>
